{
  "name": "ungit",
  "author": "Fredrik Nor√©n <fredrik.jw.noren@gmail.com>",
  "description": "Git made easy",
  "version": "1.5.20",
  "ungitPluginApiVersion": "0.2.0",
  "scripts": {
    "start": "node ./bin/ungit",
    "inspect": "node --inspect ./source/server.js",
    "test": "npm run unittest && npm run clicktest",
    "unittest": "mocha --config .mochatest.json",
    "clicktest": "mocha --config .mochaclicktest.json",
    "coverage": "nyc npm run unittest",
    "lint": "eslint . bin/*",
    "format": "npm run lint -- --fix",
    "build": "node ./scripts/build.js",
    "watch": "nodemon -C --exec \"npm run build\" -e js,less -w public/source -w public/less -w components/ -i \"*.bundle.js\"",
    "bumpdependencies": "ncu --upgrade --reject bootstrap",
    "electronpackage": "node ./scripts/electronpackage.js",
    "electronzip": "node ./scripts/electronzip.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FredrikNoren/ungit.git"
  },
  "bin": {
    "ungit": "./bin/ungit",
    "0ungit-credentials-helper": "./bin/credentials-helper"
  },
  "dependencies": {
    "@primer/octicons": "~16.3.0",
    "blueimp-md5": "~2.19.0",
    "body-parser": "~1.19.1",
    "bootstrap": "~3.4.1",
    "cookie-parser": "~1.4.6",
    "crossroads": "~0.12.2",
    "diff2html": "~3.4.14",
    "dnd-page-scroll": "0.0.4",
    "express": "~4.17.2",
    "express-session": "~1.17.2",
    "getmac": "~5.20.0",
    "hasher": "~1.2.0",
    "ignore": "~5.2.0",
    "jquery": "~3.6.0",
    "jquery-ui": "~1.13.1",
    "just-detect-adblock": "~1.1.0",
    "knockout": "~3.5.1",
    "latest-version": "~6.0.0",
    "lodash": "~4.17.21",
    "memorystore": "~1.6.6",
    "mkdirp": "~1.0.4",
    "moment": "~2.29.1",
    "node-cache": "~5.1.2",
    "node-watch": "^0.7.3",
    "nprogress": "~0.2.0",
    "open": "~8.4.0",
    "p-limit": "~4.0.0",
    "passport": "~0.5.2",
    "passport-local": "~1.0.0",
    "raven-js": "~3.27.2",
    "rc": "~1.2.8",
    "rimraf": "~3.0.2",
    "semver": "~7.3.5",
    "serve-static": "~1.14.2",
    "signals": "~1.0.0",
    "snapsvg": "~0.5.1",
    "socket.io": "~4.4.1",
    "temp": "~0.9.4",
    "tsify": "^5.0.4",
    "typescript": "^4.5.4",
    "winston": "~3.5.0",
    "yargs": "~17.3.1"
  },
  "devDependencies": {
    "@homer0/prettier-plugin-jsdoc": "^4.0.6",
    "archiver": "~5.3.0",
    "browserify": "~17.0.0",
    "dedent": "~0.7.0",
    "electron": "~15.2.0",
    "electron-packager": "~15.4.0",
    "eslint": "~8.0.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-mocha": "~9.0.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "exorcist": "~2.0.0",
    "expect.js": "~0.3.1",
    "less": "~4.1.2",
    "mocha": "~9.1.3",
    "nodemon": "~2.0.13",
    "npm-check-updates": "~11.8.5",
    "nyc": "~15.1.0",
    "portfinder": "^1.0.28",
    "prettier": "~2.5.1",
    "puppeteer": "~13.1.2",
    "superagent": "~7.1.1",
    "supertest": "~6.2.2"
  },
  "engines": {
    "node": "^10.18.0 || >=11.14.0"
  },
  "license": "MIT",
  "main": "public/main.js"
}
