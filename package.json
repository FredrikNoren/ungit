{
  "name": "ungit",
  "author": "Fredrik Nor√©n <fredrik.jw.noren@gmail.com>",
  "description": "Git made easy",
  "version": "1.5.28",
  "ungitPluginApiVersion": "0.2.0",
  "scripts": {
    "start": "node ./bin/ungit",
    "inspect": "node --inspect ./source/server.js",
    "test": "npm run unittest && npm run clicktest",
    "unittest": "mocha --config .mochatest.json",
    "clicktest": "mocha --config .mochaclicktest.json",
    "coverage": "nyc npm run unittest",
    "lint": "eslint .",
    "format": "npm run lint -- --fix",
    "build": "node ./scripts/build.js",
    "watch": "nodemon -C --exec \"npm run build\" -e js,less -w public/source -w public/less -w components/ -i \"*.bundle.js\"",
    "bumpdependencies": "ncu --upgrade --reject bootstrap",
    "electronpackage": "node ./scripts/electronpackage.js",
    "electronzip": "node ./scripts/electronzip.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FredrikNoren/ungit.git"
  },
  "bin": {
    "ungit": "./bin/ungit",
    "0ungit-credentials-helper": "./bin/credentials-helper"
  },
  "dependencies": {
    "@primer/octicons": "~19.14.0",
    "blueimp-md5": "~2.19.0",
    "body-parser": "~1.20.3",
    "bootstrap": "~3.4.1",
    "chokidar": "~3.6.0",
    "cookie-parser": "~1.4.7",
    "crossroads": "~0.12.2",
    "diff2html": "~3.4.51",
    "dnd-page-scroll": "0.0.4",
    "express": "~4.21.2",
    "express-session": "~1.18.1",
    "getmac": "~6.6.0",
    "hasher": "~1.2.0",
    "ignore": "~7.0.3",
    "jquery": "~3.7.1",
    "jquery-ui": "~1.14.1",
    "just-detect-adblock": "~1.1.0",
    "knockout": "~3.5.1",
    "latest-version": "~9.0.0",
    "lodash": "~4.17.21",
    "memorystore": "~1.6.7",
    "mkdirp": "~3.0.1",
    "moment": "~2.30.1",
    "node-cache": "~5.1.2",
    "nprogress": "~0.2.0",
    "open": "~10.1.0",
    "p-limit": "~6.2.0",
    "passport": "~0.7.0",
    "passport-local": "~1.0.0",
    "raven-js": "~3.27.2",
    "rc": "~1.2.8",
    "rimraf": "~5.0.10",
    "semver": "~7.6.3",
    "serve-static": "~1.16.2",
    "signals": "~1.0.0",
    "snapsvg": "~0.5.1",
    "socket.io": "~4.8.1",
    "temp": "~0.9.4",
    "tsify": "~5.0.4",
    "typescript": "~5.7.3",
    "winston": "~3.17.0",
    "yargs": "~17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "~9.19.0",
    "@homer0/prettier-plugin-jsdoc": "~9.1.0",
    "archiver": "~7.0.1",
    "browserify": "~17.0.1",
    "dedent": "~1.5.3",
    "electron": "~34.0.1",
    "electron-packager": "~17.1.2",
    "eslint": "~9.19.0",
    "eslint-plugin-mocha": "~10.5.0",
    "eslint-plugin-n": "~17.15.1",
    "eslint-plugin-prettier": "~5.2.3",
    "exorcist": "~2.0.0",
    "expect.js": "~0.3.1",
    "globals": "~15.14.0",
    "less": "~4.2.2",
    "mocha": "~11.1.0",
    "nodemon": "~3.1.9",
    "npm-check-updates": "~17.1.14",
    "nyc": "~17.1.0",
    "portfinder": "~1.0.32",
    "prettier": "~3.4.2",
    "puppeteer": "~24.1.1",
    "superagent": "~10.1.1",
    "supertest": "~7.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "main": "public/main.js"
}
