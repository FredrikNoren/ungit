{
  "name": "ungit",
  "author": "Fredrik Nor√©n <fredrik.jw.noren@gmail.com>",
  "description": "Git made easy",
  "version": "1.5.23",
  "ungitPluginApiVersion": "0.2.0",
  "scripts": {
    "start": "node ./bin/ungit",
    "inspect": "node --inspect ./source/server.js",
    "test": "npm run unittest && npm run clicktest",
    "unittest": "mocha --config .mochatest.json",
    "clicktest": "mocha --config .mochaclicktest.json",
    "coverage": "nyc npm run unittest",
    "lint": "eslint . bin/*",
    "format": "npm run lint -- --fix",
    "build": "node ./scripts/build.js",
    "watch": "nodemon -C --exec \"npm run build\" -e js,less -w public/source -w public/less -w components/ -i \"*.bundle.js\"",
    "bumpdependencies": "ncu --upgrade --reject bootstrap",
    "electronpackage": "node ./scripts/electronpackage.js",
    "electronzip": "node ./scripts/electronzip.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FredrikNoren/ungit.git"
  },
  "bin": {
    "ungit": "./bin/ungit",
    "0ungit-credentials-helper": "./bin/credentials-helper"
  },
  "dependencies": {
    "@primer/octicons": "~19.4.0",
    "blueimp-md5": "~2.19.0",
    "body-parser": "~1.20.2",
    "bootstrap": "~3.4.1",
    "cookie-parser": "~1.4.6",
    "crossroads": "~0.12.2",
    "diff2html": "~3.4.35",
    "dnd-page-scroll": "0.0.4",
    "express": "~4.18.2",
    "express-session": "~1.17.3",
    "getmac": "~5.20.0",
    "hasher": "~1.2.0",
    "ignore": "~5.2.4",
    "jquery": "~3.7.0",
    "jquery-ui": "~1.13.2",
    "just-detect-adblock": "~1.1.0",
    "knockout": "~3.5.1",
    "latest-version": "~7.0.0",
    "lodash": "~4.17.21",
    "memorystore": "~1.6.7",
    "mkdirp": "~3.0.1",
    "moment": "~2.29.4",
    "node-cache": "~5.1.2",
    "node-watch": "~0.7.3",
    "nprogress": "~0.2.0",
    "open": "~9.1.0",
    "p-limit": "~4.0.0",
    "passport": "~0.6.0",
    "passport-local": "~1.0.0",
    "raven-js": "~3.27.2",
    "rc": "~1.2.8",
    "rimraf": "~5.0.1",
    "semver": "~7.5.3",
    "serve-static": "~1.15.0",
    "signals": "~1.0.0",
    "snapsvg": "~0.5.1",
    "socket.io": "~4.7.1",
    "temp": "~0.9.4",
    "tsify": "~5.0.4",
    "typescript": "~5.1.6",
    "winston": "~3.9.0",
    "yargs": "~17.7.2"
  },
  "devDependencies": {
    "@homer0/prettier-plugin-jsdoc": "~6.0.5",
    "archiver": "~5.3.1",
    "browserify": "~17.0.0",
    "dedent": "~0.7.0",
    "electron": "~25.2.0",
    "electron-packager": "~17.1.1",
    "eslint": "~8.43.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-mocha": "~10.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "exorcist": "~2.0.0",
    "expect.js": "~0.3.1",
    "less": "~4.1.3",
    "mocha": "~10.2.0",
    "nodemon": "~2.0.22",
    "npm-check-updates": "~16.10.13",
    "nyc": "~15.1.0",
    "portfinder": "~1.0.32",
    "prettier": "~2.8.8",
    "puppeteer": "~20.7.3",
    "superagent": "~8.0.9",
    "supertest": "~6.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "main": "public/main.js"
}
