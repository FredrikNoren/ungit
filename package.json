{
  "name": "ungit",
  "author": "Fredrik Nor√©n <fredrik.jw.noren@gmail.com>",
  "description": "Git made easy",
  "version": "1.5.16",
  "ungitPluginApiVersion": "0.2.0",
  "scripts": {
    "start": "node ./bin/ungit",
    "inspect": "node --inspect ./source/server.js",
    "test": "npm run unittest && npm run clicktest",
    "unittest": "mocha --config .mochatest.json",
    "clicktest": "mocha --config .mochaclicktest.json",
    "clicktest-parallel": "mocha-parallel-tests --config .mochaclicktest.json",
    "coverage": "nyc npm run unittest",
    "lint": "eslint . bin/*",
    "format": "npm run lint -- --fix",
    "build": "node ./scripts/build.js",
    "watch": "nodemon -C --exec \"npm run build\" -e js,less -w public/source -w public/less -w components/ -i \"*.bundle.js\"",
    "bumpdependencies": "ncu --upgrade --reject bootstrap",
    "electronpackage": "node ./scripts/electronpackage.js",
    "electronzip": "node ./scripts/electronzip.js",
    "travisnpmpublish": "node ./scripts/travisnpmpublish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FredrikNoren/ungit.git"
  },
  "bin": {
    "ungit": "./bin/ungit",
    "0ungit-credentials-helper": "./bin/credentials-helper"
  },
  "dependencies": {
    "@primer/octicons": "~13.0.0",
    "blueimp-md5": "~2.18.0",
    "body-parser": "~1.19.0",
    "bootstrap": "~3.4.1",
    "cookie-parser": "~1.4.5",
    "crossroads": "~0.12.2",
    "diff2html": "~3.4.3",
    "dnd-page-scroll": "0.0.4",
    "express": "~4.17.1",
    "express-session": "~1.17.1",
    "getmac": "~5.17.0",
    "hasher": "~1.2.0",
    "ignore": "~5.1.8",
    "jquery": "~3.6.0",
    "jquery-ui": "~1.12.1",
    "just-detect-adblock": "~1.1.0",
    "knockout": "~3.5.1",
    "latest-version": "~5.1.0",
    "locks": "~0.2.2",
    "lodash": "~4.17.21",
    "memorystore": "~1.6.6",
    "mkdirp": "~1.0.4",
    "moment": "~2.29.1",
    "node-cache": "~5.1.2",
    "nprogress": "~0.2.0",
    "open": "~8.0.8",
    "passport": "~0.4.1",
    "passport-local": "~1.0.0",
    "raven-js": "~3.27.2",
    "rc": "~1.2.8",
    "rimraf": "~3.0.2",
    "semver": "~7.3.5",
    "serve-static": "~1.14.1",
    "signals": "~1.0.0",
    "snapsvg": "~0.5.1",
    "socket.io": "~4.0.2",
    "temp": "~0.9.4",
    "winston": "~3.3.3",
    "yargs": "~17.0.1"
  },
  "devDependencies": {
    "archiver": "~5.3.0",
    "browserify": "~17.0.0",
    "dedent": "~0.7.0",
    "electron": "~12.0.7",
    "electron-packager": "~15.2.0",
    "eslint": "~7.26.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-mocha": "~8.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.4.0",
    "exorcist": "~2.0.0",
    "expect.js": "~0.3.1",
    "less": "~4.1.1",
    "mocha": "~8.4.0",
    "mocha-parallel-tests": "~2.3.0",
    "nodemon": "~2.0.7",
    "npm-check-updates": "~11.5.10",
    "nyc": "~15.1.0",
    "prettier": "~2.3.0",
    "puppeteer": "~9.1.1",
    "superagent": "~6.1.0",
    "supertest": "~6.1.3"
  },
  "engines": {
    "node": "^10.18.0 || >=11.14.0"
  },
  "license": "MIT",
  "main": "public/main.js"
}
